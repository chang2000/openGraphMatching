   "source": [
    "import pylab\n",
    "import networkx as nx\n",
    "G=nx.Graph()\n",
    "G.add_node('Golf',size='small')\n",
    "G.add_node('Hummer',size='huge')\n",
    "G.add_edge('Golf','Hummer')\n",
    "labels = nx.get_node_attributes(G, 'size') \n",
    "nx.draw(G,labels=labels,node_size=1000)\n",
    "pylab.show()\n"
   ]
  }
